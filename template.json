    "endpoints": {
        "search": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "keywords": "{query}",
                "num_results": "20",
                "response_groups": "{responseGroups}"
            }
        },
        "bookDetails": {
            "path": "/1.0/catalog/products/{id}",
            "method": "GET",
            "queryParams": {
                "response_groups": "{responseGroups}"
            }
        },
        "categoryBooks": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "category_id": "{categoryId}",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "author": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "author": "{authorId}",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "series": {
            "path": "/1.0/catalog/products/{bookId}/sims",
            "method": "GET",
            "queryParams": {
                "similarity_type": "InTheSameSeries",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "chartsBestsellers": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "products_sort_by": "BestSellers",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "chartsTrending": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "products_sort_by": "AvgRating",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "newReleases": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "products_sort_by": "-ReleaseDate",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        },
        "comingSoon": {
            "path": "/1.0/catalog/products",
            "method": "GET",
            "queryParams": {
                "products_sort_by": "ProductSiteLaunchDate",
                "num_results": "50",
                "response_groups": "{responseGroups}"
            }
        }
    },
